{
  "name": "wordpress-ajax-filter-posts",
  "version": "0.4.0",
  "description": "Filter and load more WordPress posts via AJAX",
  "main": "index.js",
  "scripts": {
    "create-pot": "mkdir -p ./languages && find . ./templates -iname '*.php' | xargs xgettext --add-comments=TRANSLATORS --force-po --from-code=UTF-8 --default-domain=ajax-filter-posts -k__ -k_e -k_n:1,2 -k_x:1,2c -k_ex:1,2c -k_nx:4c,12 -kesc_attr__ -kesc_attr_e -kesc_attr_x:1,2c -kesc_html__ -kesc_html_e -kesc_html_x:1,2c -k_n_noop:1,2 -k_nx_noop:3c,1,2, -k__ngettext_noop:1,2 -o languages/ajax-filter-posts.pot",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robbertdk/wordpress-ajax-filter-posts.git"
  },
  "keywords": [
    "Wordpress",
    "AJAX",
    "posts",
    "filter",
    "load",
    "more",
    "pagination"
  ],
  "author": "Robbert de Kuiper",
